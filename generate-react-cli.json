{
	"usesTypeScript": true,
	"usesStyledComponents": true,
	"testLibrary": "Testing Library",
	"component": {
		"default": {
			"path": "src/components",
			"withStyle": true,
			"withTest": true,
			"withStory": true,
			"withLazy": false,
			"customTemplates": {
				"component": "./templates/TemplateName.tsx",
				"style": "./templates/TemplateName.style.ts",
				"test": "./templates/TemplateName.test.tsx",
				"story": "./templates/TemplateName.stories.ts"
			}
		},
		"doc": {
			"path": "docs/components",
			"withStyle": true,
			"withTest": false,
			"withStory": false,
			"withLazy": false,
			"customTemplates": {
				"component": "./templates/TemplateName.doc.tsx",
				"style": "./templates/TemplateName.doc.style.ts"
			}
		}
	}
}
